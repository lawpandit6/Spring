package in.ashokit.beans;

public class Car {
	
	private IEngine eng;
	
	public void setEng(IEngine eng) {
		this.eng=eng;
	}
	public Car() {
		
	}
	public void drive() {
		int start = eng.start();
		if(start>=1) {
			System.out.println("Journey started..");
		}else {
			System.out.println("Engine in trouble..");
		}
	}
}
